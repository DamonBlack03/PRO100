<Window x:Class="BoardDisplay.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoardDisplay"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="500"
        Loaded="Window_Loaded">
    <Canvas Name="Canvas_Main" Width="500" Height="500">
        <Grid Panel.ZIndex="2" x:Name="MoveBoard" Width="500" Height="500"/>
        <Grid Panel.ZIndex="1" Width="500" Height="500" Name="PieceBoard"/>
        <Grid Panel.ZIndex="0" x:Name="DisplayBoard" Width="500" Height="500"/>
    </Canvas>
</Window>
